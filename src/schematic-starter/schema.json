{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsStarter",
  "title": "Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the componet",
      "default": "ng-conf",
      "x-prompt": "Enter the name of the component"
    },
    "browserType": {
      "enum": ["netscape", "internetExplorer"],
      "description": "Supported browser",
      "default": "netscape",
      "x-prompt": "Select the supported browser"
    }
  },
  "required": [],
  "additionalProperties": false
}
